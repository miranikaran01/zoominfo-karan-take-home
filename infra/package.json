{
    "name": "karan-take-home-infra",
    "version": "0.1.0",
    "bin": {
      "karan-take-home-infra": "bin/app.js"
    },
    "scripts": {
      "build": "tsc",
      "watch": "tsc -w",
      "cdk": "npx aws-cdk",
      "deploy": "npm run build && npx aws-cdk deploy",
      "synth": "npm run build && npx aws-cdk synth",
      "bootstrap": "npx aws-cdk bootstrap"
    },
    "devDependencies": {
      "@types/node": "22.13.10",
      "aws-cdk-lib": "2.224.0",
      "constructs": "10.4.3",
      "typescript": "5.8.2"
    }
  }
  